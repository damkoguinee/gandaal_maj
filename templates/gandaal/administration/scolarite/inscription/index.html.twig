{% extends 'base.html.twig' %}

{% block title %}Inscription index{% endblock %}

{% block body %}
    <h1>Inscription index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Type</th>
                <th>RemiseInscription</th>
                <th>RemiseScolarite</th>
                <th>Promo</th>
                <th>DateInscription</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for inscription in inscriptions %}
            <tr>
                <td>{{ inscription.id }}</td>
                <td>{{ inscription.type }}</td>
                <td>{{ inscription.remiseInscription }}</td>
                <td>{{ inscription.remiseScolarite }}</td>
                <td>{{ inscription.promo }}</td>
                <td>{{ inscription.dateInscription ? inscription.dateInscription|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('app_gandaal_administration_scolarite_inscription_show', {'id': inscription.id}) }}">show</a>
                    <a href="{{ path('app_gandaal_administration_scolarite_inscription_edit', {'id': inscription.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="7">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_gandaal_administration_scolarite_inscription_new') }}">Create new</a>
{% endblock %}
